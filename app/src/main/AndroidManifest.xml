<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.campuscanteen">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_grouplogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_grouplogo"
        android:supportsRtl="true"
        android:theme="@style/Theme.CampusCanteen">
        <activity android:name=".splashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddMenuSeller" />
        <activity android:name=".MenuCanteen" />
        <activity android:name=".profile" />
        <activity android:name=".dashboardSeller" />
        <activity android:name=".regSeller" />
        <activity android:name=".registerAs" />
        <activity android:name=".dashboard" />
        <activity android:name=".Register" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>